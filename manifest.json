{
	"manifest_version": 2,
	"name": "Old Google",
	"version": "2.1.2",
	"description": "Replaces the current Google logo on google.com and the search page to use the old 2010-2013 logo.",
	"browser_specific_settings": {
		"gecko": {
			"id": "old-google@toydotgame.net",
			"strict_min_version": "53.0"
		}
	},
	"icons": {
		"32": "icons/32.png",
		"48": "icons/48.png",
		"96": "icons/96.png"
	},
	"web_accessible_resources": [
		"resources/favicon.ico",
		"resources/maps.ico",
		"resources/books.png",
		"resources/g.png",
		"resources/logo.png",
		"resources/maps.png",
		"resources/maps_watermark.png",
		"resources/maps_watermark_mono.png",
		"resources/patents.png",
		"resources/scholar.png",
		"resources/videos.png"
	],
	"browser_action": {
		"default_icon": "icons/vector.svg",
		"default_popup": "config/popup.html"
	},
	"permissions": ["storage"],
	"content_scripts": [
		{
			"matches": [
				"*://*.google.com/*"
			],
			"js": ["config/configurator.js", "replace.js"],
			"run_at": "document_start"
		}
	]
}
